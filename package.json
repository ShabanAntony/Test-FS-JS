{
  "name": "@server/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "run-p prepare:*",
    "prepare:husky": "husky install",
    "prepare:env": "ts-node --project ./tools/tsconfig.tools.json tools/scaffolding/index.ts",
    "tests:unit": "npx nx run-many --target=unit-tests --all --parallel=false --watch=false --ci=true --runInBand=true --coverage --verbose",
    "tests:types": "npx nx run-many --target=type-check",
    "openapi:generate-bff": "npx nx run bff-api:generate",
    "dev": "run-p dev:*",
    "dev:apps": "npx nx run-many --target=serve --verbose",
    "lint": "npx nx run-many --target=lint",
    "lint:fix": "npm run lint -- --fix && npx prettier --write .",
    "start": "node dist/apps/server/main.js",
    "build": "npx nx run-many --target=build --verbose --configuration=production",
    "build:debug": "cross-env NODE_ENV=production npm run build --skip-nx-cache --debug"
  },
  "prisma": {
    "schema": "./apps/server/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./apps/server/prisma/seed.ts"
  },
